CS_LAST_WORKSPACE: dotfiles
CS_WORKSPACE_BEGIN: vim
CS_WORKSPACE_END: vim

CS_WORKSPACE_BEGIN: dotfiles
let SessionLoad = 1
if &cp | set nocp | endif
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
exe "cd " . escape(expand("<sfile>:p:h"), ' ')
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 vim/neobundles.vim
badd +1 vim/tabs.vim
badd +1 vimrc_debug
badd +1 vim/perso.vim
badd +1 .git/cs_workspaces
badd +1 vim/styling.vim
badd +1 vim/test.vim
badd +1 tmuxifier/layouts/main.session.sh
badd +1 ~/.tmux.conf
badd +1 tmuxifier/layouts/issues.window.sh
badd +1 tmuxifier/layouts/launcher.window.sh
badd +1 tmuxifier/layouts/dotfiles.session.sh
badd +1 ~/.bash_aliases
badd +1 ~/.bashrc
badd +1 vimrc
badd +1 tmuxifier/layouts/missions.window.sh
badd +1 setup.sh
badd +1 ~/.bash_profile
argglobal
silent! argdel *
edit vim/neobundles.vim
edit vim/tabs.vim
edit vimrc_debug
edit vim/perso.vim
edit .git/cs_workspaces
edit vim/styling.vim
edit vim/test.vim
edit vimrc
let t:ctrlspace_label = 'Vim'
edit vim/neobundles.vim
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
argglobal
let s:l = 17 - ((15 * winheight(0) + 24) / 48)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
17
normal! 01|
tabedit ~/.tmux.conf
edit tmuxifier/layouts/main.session.sh
edit /root/.tmux.conf
edit tmuxifier/layouts/issues.window.sh
edit tmuxifier/layouts/launcher.window.sh
edit tmuxifier/layouts/dotfiles.session.sh
edit tmuxifier/layouts/missions.window.sh
let t:ctrlspace_label = 'Tmux'
edit ~/.tmux.conf
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
argglobal
let s:l = 49 - ((40 * winheight(0) + 25) / 50)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
49
normal! 050|
tabedit ~/.bashrc
edit /root/.bash_aliases
edit /root/.bashrc
edit /root/.bash_profile
edit setup.sh
let t:ctrlspace_label = 'Bash'
edit ~/.bashrc
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
argglobal
let s:l = 16 - ((14 * winheight(0) + 25) / 50)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
16
normal! 015|
tabnext 2
if exists('s:wipebuf')
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 shortmess=filnxtToOc
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
CS_WORKSPACE_END: dotfiles
