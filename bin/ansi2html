#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ansi2html==1.0.6','console_scripts','ansi2html'
__requires__ = 'ansi2html==1.0.6'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('ansi2html==1.0.6', 'console_scripts', 'ansi2html')()
    )
